/* pages/address/address.wxss */
@import '../../utils/weui.wxss';
page {
  display: flex;
  flex-direction: column;
  height: 100%;
  margin-top: 160rpx;
}

.weui-cells:before {
  top: 0;
  border-top: 1rpx solid white;
}

.weui-cell {
  line-height: 3.5rem;
}

.weui-cells:after {
  bottom: 0;
  border-bottom: 1rpx solid white;
}

.weui-btn {
  width: 80%;
}
